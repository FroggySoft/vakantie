<?xml version="1.0" encoding="utf-8"?>
<TableLayout android:id="@+id/TableLayout01" 
		xmlns:android="http://schemas.android.com/apk/res/android" 
		android:layout_gravity="center|clip_horizontal" android:background="#FFFFFFFF" android:layout_width="fill_parent" android:layout_height="fill_parent">
		
	<TableRow android:layout_marginTop="60.0px" android:layout_marginLeft="20.0px" android:layout_marginRight="20.0px">		
		<TextView 
	 		android:layout_width="wrap_content" android:layout_height="wrap_content" 
	 		android:text="Jaar:"  android:textColor="#FF000000"/>
	 		android:layout_gravity="center_vertical|center_horizontal" /> 
 	 	<EditText android:layout_width="wrap_content" android:layout_height="wrap_content" 
 	 		android:id="@+id/EditJaar" android:maxWidth="100.0px" android:maxLines="1" 
 	 		android:text="2010" android:inputType="numberDecimal"  android:textColor="#FF000000"/>/>
 	</TableRow>
	<TableRow android:layout_marginTop="20.0px" android:layout_marginBottom="60.0px" android:layout_marginLeft="20.0px" android:layout_marginRight="20.0px">		
	 	<TextView android:text="@string/Feestdagen"  
	 		android:layout_width="fill_parent" android:layout_height="wrap_content" 
	 		android:editable="false" android:minLines="6"  android:textColor="#FF000000"/>  		 
	 		android:layout_marginLeft="20.0px" android:layout_marginRight="20.0px"/>
	 	<TextView android:id="@+id/TextView01" 
 			android:layout_width="fill_parent" android:layout_height="wrap_content" 
	 		android:editable="false" android:minLines="6" 
	 		android:layout_marginRight="20.0px"  android:textColor="#FF000000"/>/> 
 	</TableRow>
</TableLayout>
	